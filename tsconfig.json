{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "assets/*": [
        "./src/assets/*"
      ],
      "styled/*": [
        "./src/style/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    },
    "skipLibCheck": true
  }
}